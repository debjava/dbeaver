    <extension point="org.eclipse.ui.popupMenus">
        <objectContribution
                adaptable="false"
                id="org.jkiss.dbeaver.core.dbsObjectContribution"
                objectClass="org.jkiss.dbeaver.model.navigator.DBNNode">
            <action
                    class="org.jkiss.dbeaver.ui.actions.OpenObjectEditorAction"
                    icon="/icons/tree/edit_item.png"
                    id="org.jkiss.dbeaver.ui.actions.OpenObjectEditorAction"
                    label="Edit Object">
            </action>
        </objectContribution>
        <objectContribution
                adaptable="false"
                id="org.jkiss.dbeaver.core.dataSourceContribution"
                objectClass="org.jkiss.dbeaver.model.navigator.DBNNode">
            <filter
                    name="targetType"
                    value="org.jkiss.dbeaver.model.struct.DBSDataSourceContainer">

            </filter>
            <action
                    class="org.jkiss.dbeaver.ui.actions.DisconnectAction"
                    icon="icons/sql/disconnect.png"
                    id="org.jkiss.dbeaver.ui.actions.DisconnectAction"
                    label="Disconnect">
                <enablement>
                    <objectState name="connected" value="true"/>
                </enablement>
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.ConnectAction"
                    icon="icons/sql/connect.png"
                    id="org.jkiss.dbeaver.ui.actions.ConnectAction"
                    label="Connect">
                <enablement>
                    <objectState name="connected" value="false"/>
                </enablement>
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.EditConnectionAction"
                    id="org.jkiss.dbeaver.ui.actions.EditConnectionAction"
                    label="Edit Connection"
                    icon="icons/tree/edit_item.png">
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.OpenSQLEditorAction"
                    id="org.jkiss.dbeaver.ui.actions.OpenSQLEditorAction"
                    label="SQL Editor"
                    icon="icons/sql/sql_script.png">
            </action>
        </objectContribution>
    </extension>
