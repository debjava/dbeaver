<plugin>
    <extension-point id="org.jkiss.dbeaver.dataSourceProvider" name="DataSource provider"
                     schema="schema/org.jkiss.dbeaver.dataSourceProvider.exsd"/>
    <extension-point id="org.jkiss.dbeaver.databaseEditor" name="Custom Entity Editor"
                     schema="schema/org.jkiss.dbeaver.databaseEditor.exsd"/>
    <extension-point id="org.jkiss.dbeaver.dataTypeProvider" name="DataType provider"
                     schema="schema/org.jkiss.dbeaver.dataTypeProvider.exsd"/>
    <extension-point id="org.jkiss.dbeaver.dataExportProvider" name="Data Export Provider" schema="schema/org.jkiss.dbeaver.dataExportProvider.exsd"/>
    <extension
            point="org.eclipse.core.contenttype.contentTypes">
        <content-type
                base-type="org.eclipse.core.runtime.text"
                default-charset="utf-8"
                file-extensions="sql"
                id="org.jkiss.dbeaver.core.content-type-sql"
                name="SQL"
                priority="normal">
        </content-type>
    </extension>

    <extension id="application" point="org.eclipse.core.runtime.applications">
        <application
                visible="true">
            <run
                    class="org.jkiss.dbeaver.core.DBeaverApplication">
            </run>
        </application>
    </extension>

    <extension point="org.eclipse.core.expressions.propertyTesters">
        <propertyTester
                class="org.jkiss.dbeaver.ui.editors.DatabaseEditorPropertyTester"
                id="org.jkiss.dbeaver.databaseEditorPropertyTester"
                namespace="org.jkiss.dbeaver.database.editor"
                properties="active"
                type="org.jkiss.dbeaver.ui.editors.IDatabaseEditor">
        </propertyTester>
    </extension>
    <extension point="org.eclipse.ui.views">
        <view
                allowMultiple="false"
                category="org.jkiss.dbeaver.core.category"
                class="org.jkiss.dbeaver.ui.views.console.ConsoleView"
                icon="icons/properties.png"
                id="org.jkiss.dbeaver.core.consoleView"
                name="SQL Console">
        </view>
        <view
                allowMultiple="false"
                category="org.jkiss.dbeaver.core.category"
                class="org.jkiss.dbeaver.ui.views.navigator.database.DatabaseNavigatorView"
                icon="icons/database_navigator.png"
                id="org.jkiss.dbeaver.core.databaseNavigator"
                name="Database Navigator">
        </view>
        <view
                allowMultiple="false"
                category="org.jkiss.dbeaver.core.category"
                class="org.jkiss.dbeaver.ui.views.navigator.project.ProjectNavigatorView"
                icon="icons/projects.png"
                id="org.jkiss.dbeaver.core.projectNavigator"
                name="Projects">
        </view>

        <category
                id="org.jkiss.dbeaver.core.category"
                name="DBeaver Core Category">
        </category>
    </extension>

    <extension
            point="org.eclipse.ui.actionSets">
        <actionSet
                id="org.jkiss.dbeaver.core.dataSourceActionSet"
                label="DataSource actions"
                visible="true">

            <menu id="dataSourceMenu" label="Database">
                <separator name="driverGroup">
                </separator>
                <separator name="connectionGroup">
                </separator>
                <separator name="toolsGroup">
                </separator>
                <separator name="sessionGroup">
                </separator>
            </menu>
            <action
                    class="org.jkiss.dbeaver.ui.actions.NewConnectionAction"
                    icon="/icons/database_connect.png"
                    id="org.jkiss.dbeaver.ui.actions.NewConnectionAction"
                    label="New Connection"
                    menubarPath="dataSourceMenu/driverGroup"
                    tooltip="Create New Connection">
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.DriverManagerAction"
                    icon="/icons/driver_manager.png"
                    id="org.jkiss.dbeaver.ui.actions.DriverManagerAction"
                    label="Driver Manager"
                    menubarPath="dataSourceMenu/driverGroup"
                    tooltip="Open Driver Manager">
            </action>

            <action
                    class="org.jkiss.dbeaver.ui.actions.DisconnectAction"
                    icon="icons/sql/disconnect.png"
                    id="org.jkiss.dbeaver.ui.actions.DisconnectAction"
                    label="Disconnect"
                    menubarPath="dataSourceMenu/connectionGroup"
                    toolbarPath="connectionGroup"
                    tooltip="Disconnect from database">
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.ReconnectAction"
                    icon="icons/sql/reconnect.png"
                    id="org.jkiss.dbeaver.ui.actions.ReconnectAction"
                    label="Reconnect"
                    menubarPath="dataSourceMenu/connectionGroup"
                    toolbarPath="connectionGroup"
                    tooltip="Invalidate connection">
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.ConnectAction"
                    icon="icons/sql/connect.png"
                    id="org.jkiss.dbeaver.ui.actions.ConnectAction"
                    label="Connect"
                    menubarPath="dataSourceMenu/connectionGroup"
                    toolbarPath="connectionGroup"
                    tooltip="Connect to database">
            </action>

            <action
                    class="org.jkiss.dbeaver.ui.actions.OpenSQLEditorAction"
                    definitionId="org.jkiss.dbeaver.core.openSQLEditor"
                    icon="/icons/sql/sql_script.png"
                    id="org.jkiss.dbeaver.ui.actions.OpenSQLEditorAction"
                    label="Open SQL Editor"
                    menubarPath="dataSourceMenu/toolsGroup"
                    toolbarPath="toolsGroup"
                    tooltip="Open SQL Editor">
            </action>

            <action
                    class="org.jkiss.dbeaver.ui.actions.TransactionModeAction"
                    icon="/icons/sql/txn_config.png"
                    id="org.jkiss.dbeaver.ui.actions.TransactionModeAction"
                    label="Transactions mode"
                    menubarPath="dataSourceMenu/sessionGroup"
                    style="pulldown"
                    toolbarPath="sessionGroup"
                    tooltip="Transactions mode">
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.RollbackAction"
                    icon="/icons/sql/rollback.png"
                    id="org.jkiss.dbeaver.ui.actions.RollbackAction"
                    label="Rollback changes"
                    menubarPath="dataSourceMenu/sessionGroup"
                    toolbarPath="sessionGroup"
                    tooltip="Rollback changes">
            </action>
            <action
                    class="org.jkiss.dbeaver.ui.actions.CommitAction"
                    icon="/icons/sql/commit.png"
                    id="org.jkiss.dbeaver.ui.actions.CommitAction"
                    label="Commit changes"
                    menubarPath="dataSourceMenu/sessionGroup"
                    toolbarPath="sessionGroup"
                    tooltip="Commit changes">
            </action>

        </actionSet>
    </extension>

    <extension
            point="org.eclipse.ui.handlers">
        <handler commandId="org.eclipse.ui.edit.copy"
                 class="org.jkiss.dbeaver.ui.actions.handlers.NavigatorCopyHandler">
            <activeWhen>
                <with variable="selection">
                    <count value="+"/>
                    <iterate operator="and">
                        <adapt type="org.jkiss.dbeaver.model.DBPNamedObject"/>
                    </iterate>
                </with>
            </activeWhen>
        </handler>
        <handler commandId="org.eclipse.ui.edit.delete"
                 class="org.jkiss.dbeaver.ui.actions.handlers.NavigatorDeleteHandler">
            <activeWhen>
                <with variable="selection">
                    <count value="+"/>
                    <iterate operator="and">
                        <adapt type="org.jkiss.dbeaver.model.DBPDeletableObject"/>
                    </iterate>
                </with>
            </activeWhen>
        </handler>
        <handler commandId="org.eclipse.ui.file.refresh"
                 class="org.jkiss.dbeaver.ui.actions.handlers.NavigatorRefreshHandler">
            <activeWhen>
                <with variable="activePart">
                    <adapt type="org.jkiss.dbeaver.ext.ui.INavigatorModelView"/>
                </with>
            </activeWhen>
        </handler>
    </extension>

    <extension
            point="org.eclipse.ui.menus">
        <menuContribution
                allPopups="false"
                locationURI="menu:edit?before=undoredo">
            <command
                    commandId="org.eclipse.ui.edit.undo"
                    style="push">
            </command>
            <command
                    commandId="org.eclipse.ui.edit.redo"
                    style="push">
            </command>
        </menuContribution>
        <menuContribution
                allPopups="false"
                locationURI="menu:edit?before=additions">
            <command
                    commandId="org.eclipse.ui.edit.cut"
                    label="Cut"
                    style="push">
            </command>
            <command
                    commandId="org.eclipse.ui.edit.copy"
                    label="Copy"
                    style="push">
            </command>
            <command
                    commandId="org.eclipse.ui.edit.paste"
                    label="Paste"
                    style="push">
            </command>
            <command
                    commandId="org.eclipse.ui.edit.delete"
                    label="Delete"
                    style="push">
            </command>
        </menuContribution>
        <menuContribution
                allPopups="false"
                locationURI="menu:file?before=end">
            <command
                    commandId="org.eclipse.ui.file.refresh"
                    label="Refresh"
                    style="push">
            </command>
        </menuContribution>
    </extension>

    <extension point="org.eclipse.ui.bindings">
        <scheme
                description="DBeaver accelerators"
                id="DBeaverKeyScheme"
                name="DBeaver"
                parentId="org.eclipse.ui.defaultAcceleratorConfiguration">
        </scheme>

        <key
                commandId="org.jkiss.dbeaver.ui.editors.sql.run.statement"
                schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
                sequence="CTRL+Enter">
        </key>
        <key
                commandId="org.jkiss.dbeaver.ui.editors.sql.run.script"
                schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
                sequence="ALT+X">
        </key>
        <key
                commandId="org.jkiss.dbeaver.core.openSQLEditor"
                schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
                sequence="CTRL+ALT+E">
        </key>
        <key
                commandId="org.jkiss.dbeaver.ui.editors.sql.open.file"
                schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
                sequence="CTRL+O">
        </key>

        <key
                commandId="org.eclipse.ui.navigate.showInQuickMenu"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="Alt+F1">
        </key>
        <key
                commandId="org.eclipse.ui.navigate.backwardHistory"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="ALT+CTRL+ARROW_LEFT">
        </key>
        <key
                commandId="org.eclipse.ui.navigate.forwardHistory"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="ALT+CTRL+ARROW_RIGHT">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.gotoLastEditPosition"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+BS">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.goto.line"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+G">
        </key>
        <key
                commandId="org.eclipse.ui.edit.findNext"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="F3">
        </key>
        <key
                commandId="org.eclipse.ui.edit.findPrevious"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="SHIFT+F3">
        </key>
        <key
                commandId="org.eclipse.ui.navigate.openResource"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+N">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.moveLineDown"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+ARROW_DOWN">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.moveLineUp"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+ARROW_UP">
        </key>
        <key
                commandId="org.eclipse.ui.file.newQuickMenu"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="ALT+INSERT">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.copyLineUp"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+D">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.delete.line"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+Y">
        </key>
        <key
                commandId="org.eclipse.ui.edit.redo"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+Z">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.join.lines"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+J">
        </key>
        <key
                commandId="org.eclipse.ui.edit.findIncremental"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="ALT+F3">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.folding.expand"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+=">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.folding.expand_all"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+=">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.folding.expand_all"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+NUMPAD_ADD">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.folding.collapse"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+-">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.folding.collapse_all"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+-">
        </key>
        <key
                commandId="org.eclipse.ui.edit.text.folding.collapse_all"
                contextId="org.eclipse.ui.textEditorScope"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+NUMPAD_SUBTRACT">
        </key>
        <key
                commandId="org.eclipse.ui.window.preferences"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+ALT+S">
        </key>
        <key
                commandId="org.eclipse.ui.window.resetPerspective"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="SHIFT+F12">
        </key>
        <key
                commandId="org.eclipse.ui.window.maximizePart"
                contextId="org.eclipse.ui.contexts.window"
                schemeId="DBeaverKeyScheme"
                sequence="CTRL+SHIFT+F12">
        </key>
    </extension>
