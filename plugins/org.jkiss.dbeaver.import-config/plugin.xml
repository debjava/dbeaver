<plugin>
   <extension point="org.eclipse.ui.importWizards">
      <category
            id="org.jkiss.dbeaver.import_config.third-party"
            name="3rd Party Configuration">
      </category>
      <wizard
            category="ThirdParty"
            class="org.jkiss.dbeaver.import_config.products.ImportConfigSquirrel"
            id="org.jkiss.dbeaver.import_config.sqlsquirrel"
            name="SQL Squirrel">
         <description>
            SQL Squirrel Client
         </description>
      </wizard>
       <wizard
