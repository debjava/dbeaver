       <category
             id="org.jkiss.dbeaver.ext.import_config.importWizards.sampleCategory"
             name="Sample File Import">
       </category>
       <wizard
             category="org.jkiss.dbeaver.ext.import_config.importWizards.sampleCategory"
             class="org.jkiss.dbeaver.ext.import_config.importWizards.ImportWizard"
             icon="icons/sample.gif"
             id="org.jkiss.dbeaver.ext.import_config.importWizards.ImportWizard"
